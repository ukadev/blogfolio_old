{
    "name": "ukadev/blogfolio",
    "description": "Blog and portfolio System using Laravel 6",
    "keywords": ["laravel","blogfolio","theme","admin","cms","panel","administration","blog","portfolio"],
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Pazos",
            "email": "mail@ukadev.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "anlutro/l4-settings": "0.9",
        "creativeorange/gravatar": "~1.0",
        "watson/active": "4.0.0",
        "laravelcollective/html":"6.0.3",
        "laracasts/flash": "3.0.3"
    },
    "require-dev": {
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.4",
        "barryvdh/laravel-debugbar": "3.2.8"
    },
    "autoload": {
        "classmap": [
            "src/Migrations",
            "src/Models",
            "src/Controllers",
            "src/Views",
            "src/Seeds"
        ],
        "psr-4": {
            "Ukadev\\Blogfolio\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ukadev\\Blogfolio\\BlogfolioServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
